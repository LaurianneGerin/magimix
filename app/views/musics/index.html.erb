<h1> Lau's favorite musics </h1>

<% @musics.each do |music| %>
  <br>
  <%= music.title %> by <%= music.artist %>
  <%= link_to 'Details', music_path(music)%>
  <%= link_to "Delete", music_path(music),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
<%end%>

<br>
<%= link_to 'Add a song', new_music_path %>
